#. ID 7, Page 1, Line 1, Pos (20,10)
msgid "『48手の本』"
msgstr "『48手的书』"

#. ID 8, Page 1, Line 2, Pos (4,7)
msgid ""
"モゲコ\n"
"「本を読むもげ！」"
msgstr ""
"mogeko\n"
"「我要看书moge！」"

#. ID 12, Page 1, Line 16, Pos (0,0)
msgid ""
"異端モゲコ\n"
"「こ、此処は！」"
msgstr ""
"异端mogeko\n"
"「这、这里是！」"

#. ID 12, Page 1, Line 21, Pos (0,0)
msgid ""
"モゲコ\n"
"「館内ではお静かに」"
msgstr ""
"mogeko\n"
"「在图书馆内请保持安静」"

#. ID 12, Page 1, Line 25, Pos (0,0)
msgid ""
"異端モゲコ\n"
"「おい！」"
msgstr ""
"异端mogeko\n"
"「喂！」"

#. ID 12, Page 1, Line 30, Pos (0,0)
msgid ""
"モゲコ\n"
"「えっ？なんですか？」"
msgstr ""
"mogeko\n"
"「嗯？怎么了？」"

#. ID 12, Page 1, Line 33, Pos (0,0)
msgid ""
"異端モゲコ\n"
"「図書館はもげこに壊されたんじゃ\n"
"　なかったのか？」"
msgstr ""
"异端mogeko\n"
"「图书馆不是已经被mogeko\n"
"　破坏掉了吗？」"

#. ID 12, Page 1, Line 37, Pos (0,0)
msgid ""
"モゲコ\n"
"「あー、館長が事前に察知して\n"
"　ここに移転させたんですよー」"
msgstr ""
"mogeko\n"
"「啊那个啊~，馆长事前察觉到了，\n"
"　所以把图书馆转移到这个地方来啦——」"

#. ID 12, Page 1, Line 41, Pos (0,0)
msgid ""
"異端モゲコ\n"
"「･････････････。」"
msgstr ""
"异端mogeko\n"
"「……………………。」"

#. ID 12, Page 1, Line 44, Pos (0,0)
msgid ""
"異端モゲコ\n"
"「嬢ちゃん、まだ希望はありそうだぜ」"
msgstr ""
"异端mogeko\n"
"「小姐，我们还有希望呢」"

#. ID 13, Page 1, Line 2, Pos (19,11)
msgid ""
"モゲコ\n"
"「ふむふむ･･･この体位もなかなか･･･」"
msgstr ""
"mogeko\n"
"「诶嘿嘿嘿……这个体位还真是相当的……」"

#. ID 14, Page 1, Line 2, Pos (19,6)
msgid ""
"受付モゲコ\n"
"「貸し出し期限は2週間ですよ～」"
msgstr ""
"前台mogeko\n"
"「出借期限是两周时间哦~」"

#. ID 31, Page 1, Line 2, Pos (24,4)
msgid ""
"モゲコ\n"
"「生ハムおいちい！\n"
"　ナマハム=ナムサン！」"
msgstr ""
"mogeko\n"
"「生火腿真好吃！\n"
"　生火腿=菩萨！」"

#. ID 31, Page 1, Line 7, Pos (24,4)
#. Contains choice at line 2 (2 options)
msgid "生ハムをとりあげますか？"
msgstr "要夺走它的生火腿吗？"

#. ID 31, Page 1, Line 9, Pos (24,4)
#. Choice (2 options, embedded in a message)
msgid ""
"はい\n"
"いいえ"
msgstr ""
"是\n"
"否"

#. ID 31, Page 1, Line 14, Pos (24,4)
msgid ""
"モゲコ\n"
"「なにするのおおおおおお！？\n"
"　かえしてええええええええええ！！！」"
msgstr ""
"mogeko\n"
"「你干嘛啊！！！！！！！！！！？\n"
"　还给我啊！！！！！！！！！！！」"

#. ID 31, Page 2, Line 2, Pos (24,4)
msgid ""
"生ハムを失って悲しみに暮れるモゲコ\n"
"「えーん！\n"
"　えーーーーん！」"
msgstr ""
"失去生火腿而感到无比伤心的mogeko\n"
"「呜——！\n"
"　呜————————呜！」"

