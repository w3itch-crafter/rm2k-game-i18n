#. ID 31, Page 1, Line 1, Pos (5,12)
#. ID 32, Page 1, Line 1, Pos (4,12)
#. ID 33, Page 1, Line 1, Pos (3,22)
#. ID 34, Page 1, Line 1, Pos (1,17)
msgid "マネキンの　首がある……"
msgstr "人體模型的頭……"

#. ID 35, Page 1, Line 1, Pos (26,12)
#. Contains choice at line 2 (2 options)
msgid "カサが　置いてある……"
msgstr "有一把雨傘……"

#. ID 35, Page 1, Line 2, Pos (26,12)
#. Choice (2 options, embedded in a message)
msgid ""
"持っていく\n"
"そのままにしておく"
msgstr ""
"拿走\n"
"就這樣放著不管它"

#. ID 35, Page 1, Line 6, Pos (26,12)
msgid "赤いカサを　手に入れた。"
msgstr "得到一把　紅色雨傘。"

#. ID 41, Page 1, Line 4, Pos (15,16)
#. Contains choice at line 2 (3 options)
msgid "「……………」"
msgstr "「……………」"

#. ID 41, Page 1, Line 5, Pos (15,16)
#. Choice (3 options, embedded in a message)
msgid ""
"メアリー？\n"
"どうしたの？\n"
"大丈夫？"
msgstr ""
"M ary？\n"
"妳怎麼了？\n"
"妳還好吧？"

#. ID 41, Page 1, Line 9, Pos (15,16)
msgid ""
"\n"
"「………メアリー………\n"
"　わたし　メアリー……」"
msgstr ""
"\n"
"「………M ary………\n"
"　我是　M ary……」"

#. ID 41, Page 1, Line 13, Pos (15,16)
msgid ""
"\n"
"「うふ　ふふふ　ふふふふふ\n"
"　あははは　あはははははは」"
msgstr ""
"\n"
"「唔呵　呵呵呵　呵呵呵呵呵\n"
"　啊哈哈哈　啊哈哈哈哈哈哈！」"

#. ID 41, Page 1, Line 18, Pos (15,16)
msgid ""
"\n"
"「わたし　メアリーっていうの\n"
"　わたし　メアリーっていうの\n"
"　わたし　メアリーっていうの」"
msgstr ""
"\n"
"「我叫做　M ary喔！\n"
"　我叫做　M ary喔！\n"
"　我叫做　M ary喔！」"

#. ID 41, Page 1, Line 23, Pos (15,16)
msgid ""
"\n"
"「うふふふふ　ふふふふふ……」"
msgstr ""
"\n"
"「唔呵呵呵呵　呵呵呵呵呵……」"

#. ID 41, Page 1, Line 38, Pos (15,16)
msgid ""
"\n"
"「……なん　でも　ない」"
msgstr ""
"\n"
"「……沒有　事。」"

#. ID 41, Page 1, Line 41, Pos (15,16)
msgid ""
"\n"
"「…………あはっ\n"
"　どうしたの？　ドウシタノ？」"
msgstr ""
"\n"
"「…………啊哈，\n"
"　妳怎麼了？　妳怎麼了？」"

#. ID 41, Page 1, Line 44, Pos (15,16)
msgid ""
"\n"
"「あはははは　ふふふふふ」"
msgstr ""
"\n"
"「啊哈哈哈哈　呵呵呵呵呵！」"

#. ID 41, Page 1, Line 47, Pos (15,16)
msgid ""
"\n"
"「いやだなぁ　あのひと……\n"
"　うふふふ　ふふふふふふ」"
msgstr ""
"\n"
"「真討厭啊　那個傢伙……\n"
"　唔呵呵呵　呵呵呵呵呵呵！」"

#. ID 41, Page 1, Line 63, Pos (15,16)
msgid ""
"\n"
"「………うん　大丈夫」"
msgstr ""
"\n"
"「………嗯　沒事。」"

#. ID 41, Page 1, Line 66, Pos (15,16)
msgid ""
"\n"
"「……だい　じょうぶ？\n"
"　ダイジョウブ？　大丈夫？」"
msgstr ""
"\n"
"「……沒有　事？\n"
"　沒事吧？　妳還好吧？」"

#. ID 41, Page 1, Line 70, Pos (15,16)
msgid ""
"\n"
"「うふふふ　ふふふふ\n"
"　あははは　はははは」"
msgstr ""
"\n"
"「唔呵呵呵　呵呵呵呵\n"
"　啊哈哈哈　哈哈哈哈！」"

#. ID 41, Page 1, Line 74, Pos (15,16)
msgid ""
"\n"
"「イヴ　大丈夫　メアリー\n"
"　大丈夫　わたし　あははは\n"
"　うふふふふ　はははははは」"
msgstr ""
"\n"
"「Ib　沒事　M ary\n"
"　我　沒事　啊哈哈哈\n"
"　唔呵呵呵呵　哈哈哈哈哈哈！」"

#. ID 42, Page 1, Line 3, Pos (7,13)
#. ID 43, Page 1, Line 3, Pos (8,12)
#. ID 44, Page 1, Line 3, Pos (6,12)
msgid ""
"\n"
"「……イヴ……どこに行くの……？」"
msgstr ""
"\n"
"「……Ib……妳要去哪裡……？」"

#. ID 42, Page 1, Line 6, Pos (7,13)
#. ID 43, Page 1, Line 6, Pos (8,12)
#. ID 44, Page 1, Line 6, Pos (6,12)
msgid ""
"\n"
"「いっしょに　いこ？」"
msgstr ""
"\n"
"「我們一起走吧？」"

#. ID 42, Page 2, Line 5, Pos (7,13)
#. ID 46, Page 2, Line 5, Pos (29,21)
msgid ""
"\n"
"「早く　外に出たいね……\n"
"　ここにいるの　飽きちゃったし……」"
msgstr ""
"\n"
"「好想要快點到外面去唷……\n"
"　我已經在這裡待膩了……」"

#. ID 42, Page 2, Line 14, Pos (7,13)
#. ID 46, Page 2, Line 14, Pos (29,21)
msgid ""
"\n"
"「ギャリーは　今頃\n"
"　なにしてるんだろうね？」"
msgstr ""
"\n"
"「Garry現在\n"
"　在做什麼呢？」"

#. ID 42, Page 2, Line 18, Pos (7,13)
#. ID 46, Page 2, Line 18, Pos (29,21)
msgid ""
"\n"
"「ひとりで　さみしくて\n"
"　泣いてるのかな？\n"
"　早く　会えるといいね……」"
msgstr ""
"\n"
"「他會不會因為一個人太寂寞了\n"
"　而在哭啊？\n"
"　要是能快點見到他就好了呢……」"

#. ID 42, Page 2, Line 28, Pos (7,13)
#. ID 46, Page 2, Line 28, Pos (29,21)
msgid ""
"\n"
"「お人形さんあそび　したいなぁ……\n"
"　わたしが　お母さん人形\n"
"　イヴが　お父さん人形ね」"
msgstr ""
"\n"
"「好想要用人偶玩扮家家酒喔……\n"
"　我拿媽媽的人偶，\n"
"　Ib拿爸爸的人偶喔。」"

#. ID 42, Page 2, Line 38, Pos (7,13)
#. ID 46, Page 2, Line 38, Pos (29,21)
msgid ""
"\n"
"「わたし　お絵かき大好き\n"
"　色んなクレヨンで　いっぱい\n"
"　お絵かき　するんだ！」"
msgstr ""
"\n"
"「我最喜歡畫畫了。\n"
"　用各種顏色的蠟筆，\n"
"　畫好多好多的畫！」"

#. ID 42, Page 2, Line 43, Pos (7,13)
#. ID 46, Page 2, Line 43, Pos (29,21)
msgid ""
"\n"
"「結構　上手いんだから！」"
msgstr ""
"\n"
"「我很會畫喔！」"

#. ID 42, Page 2, Line 51, Pos (7,13)
#. Contains choice at line 2 (3 options)
#. ID 46, Page 2, Line 51, Pos (29,21)
#. Contains choice at line 2 (3 options)
msgid "「………………」"
msgstr "「………………」"

#. ID 42, Page 2, Line 52, Pos (7,13)
#. Choice (3 options, embedded in a message)
#. ID 46, Page 2, Line 52, Pos (29,21)
#. Choice (3 options, embedded in a message)
msgid ""
"さっきどうして　行っちゃったの？\n"
"ギャリー　無事だといいね\n"
"疲れてきたけど　がんばろうね"
msgstr ""
"剛才為什麼跑走了呢？\n"
"Garry要是沒事就好了呢\n"
"雖然有點累了，但繼續加油喔"

#. ID 42, Page 2, Line 56, Pos (7,13)
#. ID 46, Page 2, Line 56, Pos (29,21)
msgid ""
"\n"
"「えっ………？\n"
"　あぁ　あれは……ごめん\n"
"　わたしにも　わかんない……」"
msgstr ""
"\n"
"「咦………？\n"
"　啊啊，那個是……對不起，\n"
"　我也不知道……」"

#. ID 42, Page 2, Line 61, Pos (7,13)
#. ID 46, Page 2, Line 61, Pos (29,21)
msgid ""
"\n"
"「なんか……\n"
"　急にいろんな　気持ちが\n"
"　こう……なんて言えばいいのかな？」"
msgstr ""
"\n"
"「感覺上，\n"
"　突然間覺得，\n"
"　像這樣……要怎麼說才好呢？」"

#. ID 42, Page 2, Line 65, Pos (7,13)
#. ID 46, Page 2, Line 65, Pos (29,21)
msgid ""
"\n"
"「ふき出して？　きて………\n"
"　ちょっと……その………」"
msgstr ""
"\n"
"「爆發？出來………\n"
"　稍微……那個………」"

#. ID 42, Page 2, Line 69, Pos (7,13)
#. ID 46, Page 2, Line 69, Pos (29,21)
msgid ""
"\n"
"「まぁ　気にしないで！\n"
"　もう　大丈夫だから」"
msgstr ""
"\n"
"「唉，不要在意啦！\n"
"　我已經沒事了。」"

#. ID 42, Page 2, Line 76, Pos (7,13)
#. ID 46, Page 2, Line 76, Pos (29,21)
msgid ""
"\n"
"「……そうだね………\n"
"　無事なら………いいけどね」"
msgstr ""
"\n"
"「……是啊………\n"
"　要是沒事………就好了呢。」"

#. ID 42, Page 2, Line 80, Pos (7,13)
#. ID 46, Page 2, Line 80, Pos (29,21)
msgid ""
"\n"
"「でももし　無事じゃなかったら\n"
"　置いていくしか　ないよね？」"
msgstr ""
"\n"
"「但是如果他發生了什麼事，\n"
"　也只好丟下他了吧？」"

#. ID 42, Page 2, Line 84, Pos (7,13)
#. ID 46, Page 2, Line 84, Pos (29,21)
msgid ""
"\n"
"「もしもの話　ね？」"
msgstr ""
"\n"
"「如果的話啦！」"

#. ID 42, Page 2, Line 90, Pos (7,13)
#. ID 46, Page 2, Line 90, Pos (29,21)
msgid ""
"\n"
"「うん……そっか　イヴは\n"
"　ずっと歩いて　きたんだもんね」"
msgstr ""
"\n"
"「嗯……對喔，\n"
"　Ib是一直走才走到這裡來的呢。」"

#. ID 42, Page 2, Line 94, Pos (7,13)
#. ID 46, Page 2, Line 94, Pos (29,21)
msgid ""
"\n"
"「イヴは倒れたら　ダメだからね？\n"
"　一緒にここを　出るんだから！」"
msgstr ""
"\n"
"「Ib妳不可以倒下喔？\n"
"　因為我們要一起出去啊！」"

#. ID 42, Page 2, Line 98, Pos (7,13)
#. ID 46, Page 2, Line 98, Pos (29,21)
msgid ""
"\n"
"「あと少しだよ　がんばろー！」"
msgstr ""
"\n"
"「只剩一點點了喔，加油吧──！」"

#. ID 42, Page 2, Line 108, Pos (7,13)
#. ID 46, Page 2, Line 108, Pos (29,21)
msgid ""
"\n"
"「ケーキに　クッキーに\n"
"　チョコレートに………」"
msgstr ""
"\n"
"「蛋糕、餅乾，\n"
"　還有巧克力………」"

#. ID 42, Page 2, Line 112, Pos (7,13)
#. ID 46, Page 2, Line 112, Pos (29,21)
msgid ""
"\n"
"「えへへ　食べたいもの！\n"
"　色んなお菓子　早く　食べたいね！」"
msgstr ""
"\n"
"「耶嘿嘿，好想吃喔！\n"
"　好想趕快吃到很多點心喔！」"

#. ID 42, Page 2, Line 121, Pos (7,13)
#. ID 46, Page 2, Line 121, Pos (29,21)
msgid ""
"\n"
"「イヴのバラ　赤くてきれいだね……」"
msgstr ""
"\n"
"「Ib的玫瑰好紅好漂亮喔……」"

#. ID 42, Page 2, Line 124, Pos (7,13)
#. Contains choice at line 3 (2 options)
#. ID 46, Page 2, Line 124, Pos (29,21)
#. Contains choice at line 3 (2 options)
msgid ""
"\n"
"「ちょっと　触らせて？」"
msgstr ""
"\n"
"「可以讓我摸摸看嗎？」"

#. ID 42, Page 2, Line 126, Pos (7,13)
#. Choice (2 options, embedded in a message)
#. ID 46, Page 2, Line 126, Pos (29,21)
#. Choice (2 options, embedded in a message)
msgid ""
"やだ\n"
"バラをメアリーに渡す"
msgstr ""
"不要\n"
"把玫瑰給M ary"

#. ID 42, Page 2, Line 129, Pos (7,13)
#. ID 46, Page 2, Line 129, Pos (29,21)
msgid ""
"\n"
"「………ちぇ………」"
msgstr ""
"\n"
"「………嘖………」"

#. ID 42, Page 2, Line 135, Pos (7,13)
#. ID 46, Page 2, Line 135, Pos (29,21)
msgid ""
"\n"
"「わぁ　ありがとう……！\n"
"　すごく　きれい………」"
msgstr ""
"\n"
"「哇！謝謝……！\n"
"　好漂亮喔………」"

#. ID 42, Page 2, Line 139, Pos (7,13)
#. ID 46, Page 2, Line 139, Pos (29,21)
msgid ""
"\n"
"「でも　バラって　きれいなのに\n"
"　トゲがあって　危ないよね\n"
"　………………………あ」"
msgstr ""
"\n"
"「可是玫瑰花雖然漂亮，\n"
"　花莖上卻有刺，很危險呢。\n"
"　………………………啊，」"

#. ID 42, Page 2, Line 145, Pos (7,13)
#. ID 46, Page 2, Line 145, Pos (29,21)
msgid ""
"\n"
"「ごめん　イヴ\n"
"　花びら　ちぎれちゃった……」"
msgstr ""
"\n"
"「對不起，Ib，\n"
"　我把花瓣拔下來了……」"

#. ID 42, Page 2, Line 156, Pos (7,13)
#. ID 46, Page 2, Line 156, Pos (29,21)
msgid ""
"\n"
"「ここは　美術館なのに\n"
"　お客様が　全然来ないよね……」"
msgstr ""
"\n"
"「這裡明明是美術館，\n"
"　但是都沒有客人來呢……」"

#. ID 42, Page 2, Line 160, Pos (7,13)
#. ID 46, Page 2, Line 160, Pos (29,21)
msgid ""
"\n"
"「もっと　たくさん\n"
"　来ればいいのに……」"
msgstr ""
"\n"
"「要是有更多人\n"
"　能來這裡就好了……」"

#. ID 42, Page 2, Line 169, Pos (7,13)
#. ID 46, Page 2, Line 169, Pos (29,21)
msgid ""
"\n"
"「あはっ……ふふっ…………」"
msgstr ""
"\n"
"「啊哈……呵呵…………」"

#. ID 42, Page 2, Line 172, Pos (7,13)
#. ID 46, Page 2, Line 172, Pos (29,21)
msgid ""
"\n"
"「ちょっと前に　通ったとこに\n"
"　ピエロの絵が　あったよね……」"
msgstr ""
"\n"
"「我們之前經過的地方，\n"
"　有一張小丑的圖對吧……」"

#. ID 42, Page 2, Line 175, Pos (7,13)
#. ID 46, Page 2, Line 175, Pos (29,21)
msgid ""
"\n"
"「あの階段　転がってたのって\n"
"　ピエロの鼻　だったんだね」"
msgstr ""
"\n"
"「那個掉在樓梯下的東西，\n"
"　是小丑的鼻子呢。」"

#. ID 42, Page 2, Line 179, Pos (7,13)
#. ID 46, Page 2, Line 179, Pos (29,21)
msgid ""
"\n"
"「あははは………」"
msgstr ""
"\n"
"「啊哈哈哈………」"

#. ID 42, Page 2, Line 187, Pos (7,13)
#. ID 46, Page 2, Line 187, Pos (29,21)
msgid ""
"\n"
"「え？　どうして\n"
"　後ろに　つくのかって？」"
msgstr ""
"\n"
"「咦？為什麼要跟在後面？」"

#. ID 42, Page 2, Line 191, Pos (7,13)
#. ID 46, Page 2, Line 191, Pos (29,21)
#, fuzzy
msgid ""
"\n"
"「いいじゃない　別に……\n"
"　特に　理由なんてないよ」"
msgstr ""
"\n"
"「有什麼關係嘛……\n"
"　沒有什麼特別的理由喔。」"

#. ID 42, Page 2, Line 195, Pos (7,13)
#. ID 46, Page 2, Line 195, Pos (29,21)
#, fuzzy
msgid ""
"\n"
"「……………」"
msgstr ""
"\n"
"「……………」"

#. ID 42, Page 2, Line 203, Pos (7,13)
#. ID 46, Page 2, Line 203, Pos (29,21)
#, fuzzy
msgid ""
"\n"
"「イヴ　わたしと\n"
"　友達になってくれる？」"
msgstr ""
"\n"
"「Ib，妳願意當我的朋友嗎？」"

#. ID 42, Page 2, Line 207, Pos (7,13)
#. ID 46, Page 2, Line 207, Pos (29,21)
#, fuzzy
msgid ""
"\n"
"「わたしね……ずっと\n"
"　同じくらいの　女の子の友達が\n"
"　欲しかったの」"
msgstr ""
"\n"
"「我啊……一直想要\n"
"　跟我差不多大的女生朋友呢。」"

#. ID 42, Page 2, Line 212, Pos (7,13)
#. ID 46, Page 2, Line 212, Pos (29,21)
#, fuzzy
msgid ""
"\n"
"「あと　イヴのお母さんに\n"
"　会ってみたいな！」"
msgstr ""
"\n"
"「還有，我也想要見見Ib的媽媽呢！」"

#. ID 42, Page 2, Line 216, Pos (7,13)
#. ID 46, Page 2, Line 216, Pos (29,21)
#, fuzzy
msgid ""
"\n"
"「わたし\n"
"　お父さんしか　いないから……」"
msgstr ""
"\n"
"「因為我只有爸爸而已……」"

#. ID 42, Page 2, Line 225, Pos (7,13)
#. Contains choice at line 3 (2 options)
#. ID 46, Page 2, Line 225, Pos (29,21)
#. Contains choice at line 3 (2 options)
#, fuzzy
msgid ""
"\n"
"「イヴ……このスカーフ　どう思う？」"
msgstr ""
