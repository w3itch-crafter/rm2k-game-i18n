#. ID 2, Page 1, Line 2, Pos (15,5)
msgid "カギが　かかっている……"
msgstr "門鎖住了……"

#. ID 2, Page 2, Line 2, Pos (15,5)
msgid "紫のカギを　使った。"
msgstr "使用　紫色鑰匙。"

#. ID 5, Page 1, Line 3, Pos (15,14)
#. ID 14, Page 1, Line 3, Pos (16,14)
#. ID 15, Page 1, Line 3, Pos (17,14)
msgid ""
"\n"
"「イヴ　そっちじゃないよ」"
msgstr ""
"\n"
"「Ib，不是那邊啦。」"

#. ID 8, Page 1, Line 24, Pos (9,11)
#. ID 12, Page 1, Line 22, Pos (9,10)
msgid "絵の中から　物音がする……"
msgstr "畫裡有聲音傳出來……"

#. ID 8, Page 1, Line 34, Pos (9,11)
#. ID 12, Page 1, Line 32, Pos (9,10)
msgid ""
"\n"
"「なに……？　この音……\n"
"　近づいてくる……」"
msgstr ""
"\n"
"「什麼……？這個聲音……\n"
"　越來越接近了……」"

#. ID 8, Page 1, Line 66, Pos (9,11)
#. ID 12, Page 1, Line 64, Pos (9,10)
msgid ""
"\n"
"「！？」"
msgstr ""
"\n"
"「！？」"

#. ID 8, Page 1, Line 69, Pos (9,11)
#. ID 12, Page 1, Line 67, Pos (9,10)
msgid ""
"\n"
"「地面から　なにか出てきた！」"
msgstr ""
"\n"
"「有東西從地面冒出來了！」"

#. ID 8, Page 1, Line 75, Pos (9,11)
#. ID 12, Page 1, Line 73, Pos (9,10)
msgid ""
"\n"
"「な　なんか　マズイわ！\n"
"　みんな絵から　離れて！」"
msgstr ""
"\n"
"「糟、糟糕！\n"
"　大家快離開畫！」"

#. ID 8, Page 1, Line 79, Pos (9,11)
#. ID 12, Page 1, Line 77, Pos (9,10)
msgid ""
"\n"
"「イヴ！　あぶない！！」"
msgstr ""
"\n"
"「Ib！危險！！」"

#. ID 8, Page 1, Line 92, Pos (9,11)
#. ID 12, Page 1, Line 90, Pos (9,10)
msgid ""
"\n"
"「……………！」"
msgstr ""
"\n"
"「……………！」"

#. ID 8, Page 1, Line 101, Pos (9,11)
#. ID 12, Page 1, Line 99, Pos (9,10)
msgid ""
"\n"
"「ふたりとも　大丈夫！？」"
msgstr ""
"\n"
"「兩個人都沒事吧！？」"

#. ID 8, Page 1, Line 104, Pos (9,11)
#. ID 12, Page 1, Line 102, Pos (9,10)
msgid ""
"\n"
"「あー　びっくりした！」"
msgstr ""
"\n"
"「啊──，嚇了我一大跳！」"

#. ID 8, Page 1, Line 107, Pos (9,11)
#. Contains choice at line 3 (2 options)
#. ID 12, Page 1, Line 105, Pos (9,10)
#. Contains choice at line 3 (2 options)
msgid ""
"\n"
"「イヴは？　ケガとかしてない？」"
msgstr ""
"\n"
"「Ib呢？有沒有受傷？」"

#. ID 8, Page 1, Line 109, Pos (9,11)
#. Choice (2 options, embedded in a message)
#. ID 12, Page 1, Line 107, Pos (9,10)
#. Choice (2 options, embedded in a message)
msgid ""
"大丈夫\n"
"怖かった"
msgstr ""
"沒事\n"
"好可怕"

#. ID 8, Page 1, Line 112, Pos (9,11)
#. ID 12, Page 1, Line 110, Pos (9,10)
msgid ""
"\n"
"「よ……良かった……」"
msgstr ""
"\n"
"「太……太好了……」"

#. ID 8, Page 1, Line 117, Pos (9,11)
#. ID 12, Page 1, Line 115, Pos (9,10)
msgid ""
"\n"
"「たしかに　びっくりしたわね……\n"
"　でも　ケガがないなら　良かった」"
msgstr ""
"\n"
"「的確讓人嚇了一大跳呢……\n"
"　不過，沒受傷就好。」"

#. ID 8, Page 1, Line 123, Pos (9,11)
#. ID 12, Page 1, Line 121, Pos (9,10)
msgid ""
"\n"
"「それにしても\n"
"　これ……邪魔でそっちに\n"
"　行けないんだけど」"
msgstr ""
"\n"
"「話說回來，\n"
"　這東西……擋在這裡，\n"
"　這樣就沒辦法過去那邊了。」"

#. ID 8, Page 1, Line 128, Pos (9,11)
#. ID 12, Page 1, Line 126, Pos (9,10)
msgid ""
"\n"
"「折ったり　できないかしら？\n"
"　………って　なにこれ」"
msgstr ""
"\n"
"「不知道能不能折斷它？\n"
"　………咦，這是什麼？」"

#. ID 8, Page 1, Line 132, Pos (9,11)
#. ID 12, Page 1, Line 130, Pos (9,10)
msgid ""
"\n"
"「石でできてるわ　この植物」"
msgstr ""
"\n"
"「這植物是用石頭做成的啊！」"

#. ID 8, Page 1, Line 135, Pos (9,11)
#. ID 12, Page 1, Line 133, Pos (9,10)
#. ID 13, Page 1, Line 26, Pos (7,3)
msgid ""
"\n"
"「どうしましょ……」"
msgstr ""

#. ID 8, Page 1, Line 143, Pos (9,11)
#. ID 12, Page 1, Line 141, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「………ねぇ　イヴ\n"
"　さっきの部屋で　カギ拾ったよね？」"
msgstr ""
"\n"
"「該怎麼辦才好……」"

#. ID 8, Page 1, Line 148, Pos (9,11)
#. ID 12, Page 1, Line 146, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「その　カギで………」"
msgstr ""
"\n"
"「………欸，Ib，\n"
"　妳在剛才的房間裡，\n"
"　有撿到鑰匙對吧？」"

#. ID 8, Page 1, Line 153, Pos (9,11)
#. ID 12, Page 1, Line 151, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「そこのドア　開けられるんじゃない？」"
msgstr ""
"\n"
"「用那把鑰匙………」"

#. ID 8, Page 1, Line 157, Pos (9,11)
#. ID 12, Page 1, Line 155, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「もしかしたら　違う部屋に\n"
"　これを　壊せる道具が\n"
"　あるかも　しれないよ」"
msgstr ""
"\n"
"「應該能夠打開那邊那扇門吧？」"

#. ID 8, Page 1, Line 164, Pos (9,11)
#. ID 12, Page 1, Line 162, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「ねぇ　見てきていいよね？」"
msgstr ""
"\n"
"「說不定在別的房間，\n"
"　可以找到破壞這個的道具喔。」"

#. ID 8, Page 1, Line 170, Pos (9,11)
#. ID 12, Page 1, Line 168, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「うーん……でも………\n"
"　２人だけで　大丈夫かしら……」"
msgstr ""
"\n"
"「怎麼樣，我們可以去找找看吧？」"

#. ID 8, Page 1, Line 176, Pos (9,11)
#. Contains choice at line 3 (2 options)
#. ID 12, Page 1, Line 174, Pos (9,10)
#. Contains choice at line 3 (2 options)
msgid ""
"\n"
"「大丈夫よ！　ね　イヴ？」"
msgstr ""
"\n"
"「沒問題的！對吧，Ib？」"

#. ID 8, Page 1, Line 178, Pos (9,11)
#. Choice (2 options, embedded in a message)
#. ID 12, Page 1, Line 176, Pos (9,10)
#. Choice (2 options, embedded in a message)
msgid ""
"大丈夫だと思う\n"
"分かれない方が良い"
msgstr ""
"我想應該沒問題\n"
"不要分開比較好"

#. ID 8, Page 1, Line 184, Pos (9,11)
#. ID 12, Page 1, Line 182, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「ほら　イヴだって\n"
"　大丈夫って　言ってる！」"
msgstr ""
"\n"
"「嗯……可是………\n"
"　只有妳們兩個人，沒問題嗎……」"

#. ID 8, Page 1, Line 190, Pos (9,11)
#. ID 12, Page 1, Line 188, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「うーん……そ　そう………？」"
msgstr ""
"\n"
"「你看，Ib也說沒問題的！」"

#. ID 8, Page 1, Line 193, Pos (9,11)
#. ID 12, Page 1, Line 191, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「じゃあ　ちょっと\n"
"　見てきて　もらおうかしら」"
msgstr ""
"\n"
"「嗯……是、是這樣嗎………？」"

#. ID 8, Page 1, Line 199, Pos (9,11)
#. ID 12, Page 1, Line 197, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「えーっ　なんでよ？\n"
"　すぐ戻ってくれば　大丈夫だよ！」"
msgstr ""
"\n"
"「咦──？為什麼啊？\n"
"　只要馬上回來就好了啊！」"

#. ID 8, Page 1, Line 203, Pos (9,11)
#. ID 12, Page 1, Line 201, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「他に　良い方法思いつかないし……」"
msgstr ""
"\n"
"「而且也想不到其他更好的辦法了……」"

#. ID 8, Page 1, Line 206, Pos (9,11)
#. ID 12, Page 1, Line 204, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「……………」"
msgstr ""
"\n"
"「……………」"

#. ID 8, Page 1, Line 212, Pos (9,11)
#. ID 12, Page 1, Line 210, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「たしかに　その通りだわ\n"
"　……別行動は　あんまり\n"
"　気乗り　しないけど……」"
msgstr ""
"\n"
"「確實有道理，\n"
"　……雖然不太想分開行動……」"

#. ID 8, Page 1, Line 219, Pos (9,11)
#. ID 12, Page 1, Line 217, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「でも　いい？\n"
"　何もなかったら　すぐに\n"
"　ここに　戻ってくるのよ？」"
msgstr ""
"\n"
"「可是，聽好囉？\n"
"　找不到什麼有用的東西的話，\n"
"　就要馬上回來這裡喔？」"

#. ID 8, Page 1, Line 224, Pos (9,11)
#. ID 12, Page 1, Line 222, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「どうするのかは　その後\n"
"　改めて　考えましょ」"
msgstr ""
"\n"
"「至於要怎麼辦，\n"
"　之後再想吧。」"

#. ID 8, Page 1, Line 228, Pos (9,11)
#. ID 12, Page 1, Line 226, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「うん！　わかった！」"
msgstr ""
"\n"
"「嗯！我知道了！」"

#. ID 8, Page 1, Line 233, Pos (9,11)
#. ID 12, Page 1, Line 231, Pos (9,10)
#, fuzzy
msgid ""
"\n"
"「それじゃ　行こう！」"
msgstr ""
"\n"
"「那麼我們走吧！」"

#. ID 8, Page 2, Line 1, Pos (9,11)
msgid "石でできた植物が　道を塞いでいる……"
msgstr "用石頭做成的植物，封住了道路……"

#. ID 8, Page 3, Line 3, Pos (9,11)
#. ID 12, Page 3, Line 3, Pos (9,10)
msgid ""
"\n"
"「本物の　植物だったら\n"
"　燃やすって手も　あったけど……\n"
"　これはさすがに　無理だわ」"
msgstr ""
"\n"
"「如果是真正的植物，\n"
"　就可以燒掉它了……\n"
"　但這個怎樣都燒不起來吧。」"

#. ID 10, Page 1, Line 1, Pos (8,20)
msgid "『一体　どちらが　正しいのか』"
msgstr "『到底　哪一邊　才是正確的呢』"

#. ID 12, Page 2, Line 3, Pos (9,10)
msgid ""
"\n"
"「気をつけるのよ！」"
msgstr ""
"\n"
"「兩個人都要小心唷！」"

#. ID 13, Page 1, Line 5, Pos (7,3)
msgid ""
"\n"
"「………………………」"
msgstr ""
"\n"
"「………………………」"

#. ID 13, Page 1, Line 8, Pos (7,3)
msgid ""
"\n"
"「……遅いわね………\n"
"　何か　あったのかしら」"
msgstr ""
"\n"
"「……好慢哪………\n"
"　難道出了什麼事嗎。」"

#. ID 13, Page 1, Line 14, Pos (7,3)
msgid ""
"\n"
"「イヴ！　メアリー！\n"
"　聞こえたら　返事して！」"
msgstr ""
"\n"
"「Ib！　M ary！\n"
"　聽到的話就回答一下！」"

#. ID 13, Page 1, Line 21, Pos (7,3)
msgid ""
"\n"
"「駄目ね………\n"
"　あぁ　やっぱり　２人だけで\n"
"　行かせるんじゃ　なかった……！」"
msgstr ""
"\n"
"「不行呢………\n"
"　啊啊，果然不應該\n"
"　讓她們兩個自己去的啊……！」"

#. ID 13, Page 1, Line 35, Pos (7,3)
#, fuzzy
msgid ""
"\n"
"「……もう一度　あの部屋\n"
"　調べて　みようかしら」"
msgstr ""
"\n"
"「怎麼辦呢……」"

#. ID 13, Page 1, Line 39, Pos (7,3)
#, fuzzy
msgid ""
"\n"
"「……あんまり　入りたくないけど」"
msgstr ""
"\n"
"「……再進去那個房間\n"
"　重新調查一次好了。」"

#. ID 17, Page 1, Line 1, Pos (9,9)
msgid "『嫉妬深き花』"
msgstr "『善妒之花』"

